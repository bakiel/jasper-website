{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/health",
      "dest": "app/main.py"
    },
    {
      "src": "/api/docs",
      "dest": "app/main.py"
    },
    {
      "src": "/api/openapi.json",
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ]
}
